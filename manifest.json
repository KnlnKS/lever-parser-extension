{
    "manifest_version": 2,
    "name": "lever-parser-extension",
    "description": "A lever resume parser extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://*.lever.co/*/apply"
            ],
            "js": [
                "jquery.js",
                "index.js"
            ]
        }
    ]
}