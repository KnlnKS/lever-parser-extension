{
    "manifest_version": 2,
    "name": "Lever Resume Parser",
    "description": "A lever resume parser extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://*.lever.co/parse"
            ],
            "js": [
                "jquery.js",
                "index.js",
                "injectStyles.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}